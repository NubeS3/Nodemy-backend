{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-console": "off",
    "no-await-in-loop": "off",
    "no-empty": ["error", { "allowEmptyCatch": true }],
    "indent": ["warn", 2, { "MemberExpression": 0 }]
  }
}
